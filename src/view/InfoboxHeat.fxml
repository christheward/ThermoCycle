<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
       <VBox id="VBox" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Label text="Heat Flux" underline="true">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <TableView fx:id="heatTable" editable="true" prefHeight="200.0">
               <columns>
                  <TableColumn fx:id="heatNameColumn" editable="false" maxWidth="500.0" minWidth="100.0" prefWidth="100.0" text="Name" />
                  <TableColumn fx:id="heatValueColumn" maxWidth="500.0" minWidth="100.0" prefWidth="100.0" sortable="false" text="Value" />
                  <TableColumn fx:id="heatUnitsColumn" editable="false" maxWidth="150.0" minWidth="75.0" prefWidth="75.0" sortable="false" text="Units" />
                  <TableColumn fx:id="heatClearColumn" editable="false" maxWidth="100.0" minWidth="50.0" prefWidth="50.0" sortable="false" text="Clear" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
       </VBox>
    </children>
</fx:root>
