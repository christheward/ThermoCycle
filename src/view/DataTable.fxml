<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<fx:root type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <TitledPane fx:id="dataContainer" focusTraversable="false" text="Label" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <content>
            <TableView fx:id="dataTable" editable="true">
               <columns>
                  <TableColumn fx:id="nameColumn" editable="false" maxWidth="500.0" minWidth="125.0" prefWidth="-1.0" resizable="false" text="Name" />
                  <TableColumn fx:id="valueColumn" maxWidth="500.0" minWidth="75.0" prefWidth="-1.0" resizable="false" sortable="false" text="Value" />
                  <TableColumn fx:id="unitsColumn" maxWidth="150.0" minWidth="100.0" prefWidth="-1.0" resizable="false" sortable="false" text="Units" />
                  <TableColumn fx:id="setColumn" editable="false" maxWidth="100.0" minWidth="75.0" prefWidth="-1.0" resizable="false" sortable="false" text="Set" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </content>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </TitledPane>
    </children>
</fx:root>
